{
  "name": "hello-web-components",
  "version": "1.3.8",
  "type": "module",
  "description": "A simple starter hello world web component written in typescript, using lit",
  "repository": "fernandopasik/hello-web-components",
  "homepage": "https://github.com/fernandopasik/hello-web-components",
  "bugs": "https://github.com/fernandopasik/hello-web-components/issues",
  "author": "Fernando Pasik <fernando@pasik.com.ar> (https://fernandopasik.com)",
  "license": "MIT",
  "main": "hello-web-components.js",
  "module": "hello-web-components.js",
  "typings": "hello-web-components.d.ts",
  "files": [
    "/hello-web-components.*",
    "/lib"
  ],
  "keywords": [
    "hello-world",
    "lit",
    "lit-element",
    "lit-html",
    "starter",
    "web-components"
  ],
  "scripts": {
    "analyze": "wca analyze src --format json --outFile custom-elements.json",
    "build": "tsc -p tsconfig.build.json && rollup -c",
    "check-types": "tsc --noEmit",
    "clean": "del _site coverage lib hello-web-components.* index.html",
    "docs": "jekyll serve -s docs",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint . --ext js,ts",
    "lit-analyze": "lit-analyzer src --strict",
    "postpublish": "pinst --enable",
    "prebuild": "del lib hello-web-components.* index.html",
    "prepare": "husky install",
    "prepublishOnly": "pinst --disable",
    "preversion": "npm run verify",
    "size": "bundlewatch",
    "start": "start-storybook -p 6006",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:e2e": "JEST_PUPPETEER_CONFIG=jest-puppeteer.config.cjs jest",
    "verify": "npm run format:check && npm run lint && npm run lit-analyze && npm run check-types && npm run test:coverage && npm run build && npm run size"
  },
  "peerDependencies": {
    "lit": "2.x.x"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@open-wc/rollup-plugin-html": "^1.2.5",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@storybook/addon-a11y": "^6.4.19",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/web-components": "^6.4.19",
    "@types/expect-puppeteer": "^4.4.7",
    "@types/jest": "^27.4.1",
    "@types/jest-axe": "^3.5.3",
    "@types/jest-environment-puppeteer": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "@webcomponents/webcomponentsjs": "^2.6.0",
    "bundlewatch": "^0.3.3",
    "del-cli": "^4.0.1",
    "eslint": "^8.9.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-lit": "^1.6.1",
    "eslint-plugin-lit-a11y": "^2.2.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-storybook": "^0.5.7",
    "eslint-plugin-wc": "^1.3.2",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "jest-axe": "^5.0.1",
    "jest-junit": "^13.0.0",
    "jest-puppeteer": "^6.1.0",
    "lint-staged": "^12.3.4",
    "lit": "^2.2.0",
    "lit-analyzer": "^1.2.1",
    "pinst": "^3.0.0",
    "prettier": "^2.5.1",
    "prettier-plugin-organize-imports": "^2.3.4",
    "prettier-plugin-pkg": "^0.11.1",
    "prettier-plugin-sh": "^0.8.1",
    "puppeteer": "^13.4.0",
    "rollup": "^2.68.0",
    "rollup-plugin-minify-html-literals": "^1.2.6",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^27.1.3",
    "ts-lit-plugin": "^1.2.1",
    "ts-loader": "^9.2.6",
    "type-fest": "^2.12.0",
    "typescript": "^4.5.5",
    "wait-on": "^6.0.1",
    "web-component-analyzer": "^2.0.0-next.3"
  },
  "bundlewatch": {
    "files": [
      {
        "maxSize": "1 kB",
        "path": "hello-web-components.min.js"
      }
    ]
  },
  "overrides": {
    "glob-parent": "6.0.2",
    "trim": "^1.0.1"
  }
}
