{
  "name": "hello-web-components",
  "version": "1.4.0",
  "type": "module",
  "description": "A simple starter hello world web component written in typescript, using lit",
  "repository": "fernandopasik/hello-web-components",
  "homepage": "https://github.com/fernandopasik/hello-web-components",
  "bugs": "https://github.com/fernandopasik/hello-web-components/issues",
  "author": "Fernando Pasik <fernando@pasik.com.ar> (https://fernandopasik.com)",
  "license": "MIT",
  "main": "hello-web-components.js",
  "module": "hello-web-components.js",
  "typings": "hello-web-components.d.ts",
  "files": [
    "/hello-web-components.*",
    "/lib"
  ],
  "keywords": [
    "hello-world",
    "lit",
    "lit-element",
    "lit-html",
    "starter",
    "web-components"
  ],
  "scripts": {
    "analyze": "wca analyze src --format json --outFile custom-elements.json",
    "build": "tsc -p tsconfig.build.json && rollup -c",
    "check-types": "tsc --noEmit",
    "clean": "del _site coverage lib hello-web-components.* index.html",
    "docs": "jekyll serve -s docs",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "ESLINT_USE_FLAT_CONFIG=false eslint . --ext js,ts",
    "lit-analyze": "lit-analyzer src --strict",
    "prebuild": "del lib hello-web-components.* index.html",
    "prepare": "husky",
    "preversion": "npm run verify",
    "size": "size-limit",
    "storybook": "storybook dev -p 6006",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:install": "playwright install --with-deps",
    "verify": "npm run format:check && npm run lint && npm run lit-analyze && npm run check-types && npm run test:coverage && npm run build && npm run size"
  },
  "peerDependencies": {
    "lit": "3.x.x"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@jest/globals": "^29.7.0",
    "@playwright/test": "1.47.2",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@sand4rt/experimental-ct-web": "^1.47.0",
    "@size-limit/file": "^11.1.5",
    "@storybook/addon-a11y": "^8.3.2",
    "@storybook/addon-essentials": "^8.3.2",
    "@storybook/web-components": "^8.3.2",
    "@storybook/web-components-vite": "^8.3.2",
    "@types/jest": "^29.5.13",
    "@types/jest-axe": "^3.5.9",
    "@types/mime": "^4.0.0",
    "@types/node": "^22.6.1",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "@web/rollup-plugin-html": "^2.3.0",
    "@webcomponents/webcomponentsjs": "^2.8.0",
    "del-cli": "^5.1.0",
    "eslint": "^9.11.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-formatter-junit": "^8.40.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-lit": "^1.15.0",
    "eslint-plugin-lit-a11y": "^4.1.4",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-require-extensions": "^0.1.3",
    "eslint-plugin-storybook": "^0.9.0",
    "eslint-plugin-wc": "^2.1.1",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "jest-axe": "^9.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-junit": "^16.0.0",
    "lint-staged": "^15.2.10",
    "lit": "^3.2.0",
    "lit-analyzer": "^2.0.3",
    "prettier": "^3.3.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-pkg": "^0.18.1",
    "prettier-plugin-sh": "^0.14.0",
    "rollup": "^4.22.4",
    "rollup-plugin-serve": "^3.0.0",
    "size-limit": "^11.1.5",
    "storybook": "^8.3.2",
    "ts-jest": "^29.2.5",
    "ts-lit-plugin": "^2.0.2",
    "type-fest": "^4.26.1",
    "typescript": "5.5.4",
    "vite": "^5.4.7",
    "wait-on": "^8.0.1",
    "web-component-analyzer": "^2.0.0"
  },
  "overrides": {
    "eslint": "^9.11.1",
    "glob-parent": "6.0.2"
  }
}
