{
  "name": "hello-web-components",
  "version": "1.3.2",
  "description": "A simple starter hello world web component written in typescript, using lit-html and lit-element",
  "keywords": [
    "hello-world",
    "lit-element",
    "lit-html",
    "starter",
    "web-components"
  ],
  "homepage": "https://github.com/fernandopasik/hello-web-components",
  "bugs": "https://github.com/fernandopasik/hello-web-components/issues",
  "repository": "fernandopasik/hello-web-components",
  "license": "MIT",
  "author": "Fernando Pasik <fernando@pasik.com.ar> (https://fernandopasik.com)",
  "type": "module",
  "main": "hello-web-components.js",
  "module": "hello-web-components.js",
  "typings": "hello-web-components.d.ts",
  "files": [
    "/lib",
    "/hello-web-components.*"
  ],
  "scripts": {
    "analyze": "wca analyze src --format json --outFile custom-elements.json",
    "prebuild": "del lib hello-web-components.* index.html",
    "build": "tsc && rollup -c",
    "check-types": "tsc --noEmit -p tsconfig.all.json",
    "clean": "del _site coverage lib hello-web-components.* index.html",
    "docs": "jekyll serve -s docs",
    "format": "prettier --no-error-on-unmatched-pattern --write ** ./* ./.??*",
    "lint": "eslint . --ext js,ts",
    "lit-analyze": "lit-analyzer src --strict",
    "postinstall": "husky install",
    "postpublish": "pinst --enable",
    "prepublishOnly": "pinst --disable",
    "preversion": "yarn verify",
    "size": "bundlewatch",
    "start": "start-storybook -p 6006",
    "test": "jest",
    "test:e2e": "concurrently -s first -k 'SERVE=true yarn build' 'wait-on http://localhost:10001 && jest'",
    "verify": "yarn lint && yarn lit-analyze && yarn check-types && yarn test && yarn test:e2e && yarn build && yarn size"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.0.0",
    "@commitlint/config-conventional": "^13.0.0",
    "@open-wc/rollup-plugin-html": "^1.2.5",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@storybook/addon-controls": "^6.3.4",
    "@storybook/addon-docs": "^6.3.4",
    "@storybook/web-components": "^6.3.4",
    "@types/expect-puppeteer": "^4.4.6",
    "@types/jest": "^26.0.24",
    "@types/jest-axe": "^3.5.2",
    "@types/jest-environment-puppeteer": "^4.4.1",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "@webcomponents/webcomponentsjs": "^2.5.0",
    "bundlewatch": "^0.3.2",
    "codecov": "^3.8.2",
    "concurrently": "^6.2.0",
    "del-cli": "^4.0.0",
    "eslint": "^7.30.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-lit": "^1.5.1",
    "eslint-plugin-lit-a11y": "^1.1.0-next.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-wc": "^1.3.0",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "jest-axe": "^5.0.1",
    "jest-junit": "^12.2.0",
    "jest-puppeteer": "^5.0.4",
    "lint-staged": "^11.0.0",
    "lit": "^2.0.0-rc.2",
    "lit-analyzer": "^1.2.1",
    "pinst": "^2.1.6",
    "prettier": "^2.3.2",
    "prettier-plugin-organize-imports": "^2.2.0",
    "prettier-plugin-packagejson": "^2.2.11",
    "puppeteer": "^10.1.0",
    "rollup": "^2.53.1",
    "rollup-plugin-minify-html-literals": "^1.2.6",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "type-fest": "^1.2.2",
    "typescript": "^4.3.5",
    "wait-on": "^6.0.0",
    "web-component-analyzer": "^1.2.0-next.0"
  },
  "peerDependencies": {
    "lit": "2.x.x"
  },
  "bundlewatch": {
    "files": [
      {
        "maxSize": "1 kB",
        "path": "hello-web-components.min.js"
      }
    ]
  }
}
