{
  "name": "hello-web-components",
  "version": "1.3.5",
  "type": "module",
  "description": "A simple starter hello world web component written in typescript, using lit-html and lit-element",
  "repository": "fernandopasik/hello-web-components",
  "homepage": "https://github.com/fernandopasik/hello-web-components",
  "bugs": "https://github.com/fernandopasik/hello-web-components/issues",
  "author": "Fernando Pasik <fernando@pasik.com.ar> (https://fernandopasik.com)",
  "license": "MIT",
  "main": "hello-web-components.js",
  "module": "hello-web-components.js",
  "typings": "hello-web-components.d.ts",
  "files": [
    "/hello-web-components.*",
    "/lib"
  ],
  "keywords": [
    "hello-world",
    "lit-element",
    "lit-html",
    "starter",
    "web-components"
  ],
  "scripts": {
    "analyze": "wca analyze src --format json --outFile custom-elements.json",
    "build": "tsc && rollup -c",
    "check-types": "tsc --noEmit -p tsconfig.all.json",
    "clean": "del _site coverage lib hello-web-components.* index.html",
    "docs": "jekyll serve -s docs",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint . --ext js,ts",
    "lit-analyze": "lit-analyzer src --strict",
    "postpublish": "pinst --enable",
    "prebuild": "del lib hello-web-components.* index.html",
    "prepare": "husky install",
    "prepublishOnly": "pinst --disable",
    "preversion": "yarn verify",
    "size": "bundlewatch",
    "start": "start-storybook -p 6006",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:e2e": "JEST_PUPPETEER_CONFIG=jest-puppeteer.config.cjs jest",
    "verify": "yarn format:check && yarn lint && yarn lit-analyze && yarn check-types && yarn test:coverage && yarn build && yarn size"
  },
  "peerDependencies": {
    "lit": "2.x.x"
  },
  "devDependencies": {
    "@commitlint/cli": "^14.1.0",
    "@commitlint/config-conventional": "^14.1.0",
    "@open-wc/rollup-plugin-html": "^1.2.5",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@storybook/addon-a11y": "^6.3.12",
    "@storybook/addon-essentials": "^6.3.12",
    "@storybook/web-components": "^6.3.12",
    "@types/expect-puppeteer": "^4.4.7",
    "@types/jest": "^27.0.2",
    "@types/jest-axe": "^3.5.3",
    "@types/jest-environment-puppeteer": "^4.4.1",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "@webcomponents/webcomponentsjs": "^2.6.0",
    "bundlewatch": "^0.3.2",
    "codecov": "^3.8.3",
    "concurrently": "^6.3.0",
    "del-cli": "^4.0.1",
    "eslint": "^8.2.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jest": "^25.2.4",
    "eslint-plugin-lit": "^1.6.1",
    "eslint-plugin-lit-a11y": "^2.2.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-wc": "^1.3.2",
    "husky": "^7.0.4",
    "jest": "^27.3.1",
    "jest-axe": "^5.0.1",
    "jest-junit": "^13.0.0",
    "jest-puppeteer": "^6.0.0",
    "lint-staged": "^11.2.6",
    "lit": "^2.0.2",
    "lit-analyzer": "^1.2.1",
    "pinst": "^2.1.6",
    "prettier": "^2.4.1",
    "prettier-plugin-organize-imports": "^2.3.4",
    "prettier-plugin-pkg": "^0.10.0",
    "prettier-plugin-sh": "^0.7.1",
    "puppeteer": "^11.0.0",
    "rollup": "^2.59.0",
    "rollup-plugin-minify-html-literals": "^1.2.6",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.6",
    "type-fest": "^2.5.2",
    "typescript": "^4.4.4",
    "wait-on": "^6.0.0",
    "web-component-analyzer": "^2.0.0-next.3"
  },
  "resolutions": {
    "browserslist": "^4.17.1",
    "glob-parent": "^5.1.2",
    "immer": "^9.0.6",
    "prismjs": "^1.25.0",
    "set-value": "^4.1.0",
    "strip-ansi": "^6.0.1",
    "trim": "^1.0.1"
  },
  "bundlewatch": {
    "files": [
      {
        "maxSize": "1 kB",
        "path": "hello-web-components.min.js"
      }
    ]
  }
}
