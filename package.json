{
  "name": "hello-web-components",
  "version": "1.0.1",
  "description": "A simple starter hello world web component written in typescript, using lit-html and lit-element",
  "keywords": [
    "hello-world",
    "lit-element",
    "lit-html",
    "starter",
    "web-components"
  ],
  "homepage": "https://github.com/fernandopasik/hello-web-components",
  "bugs": "https://github.com/fernandopasik/hello-web-components/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/fernandopasik/hello-web-components.git"
  },
  "license": "MIT",
  "author": "Fernando Pasik <fernando@pasik.com.ar> (https://fernandopasik.com)",
  "sideEffects": false,
  "type": "module",
  "main": "hello-web-components.js",
  "module": "hello-web-components.js",
  "typings": "hello-web-components.d.ts",
  "files": [
    "/lib",
    "/hello-web-components.*"
  ],
  "scripts": {
    "analyze": "wca analyze src --format json --outFile custom-elements.json",
    "prebuild": "del lib hello-web-components.* index.html",
    "build": "tsc && rollup -c",
    "check-types": "tsc --noEmit -p tsconfig.all.json",
    "docs": "jekyll serve -s docs",
    "lint": "lit-analyzer src --strict && eslint . --ext ts,js",
    "size": "bundlesize",
    "start": "start-storybook -p 6006",
    "test": "jest",
    "test:e2e": "concurrently -s first -k 'SERVE=true yarn build' 'wait-on http://localhost:10001 && jest'",
    "verify": "yarn lint && yarn check-types && yarn test && yarn test:e2e && yarn build && yarn size",
    "preversion": "yarn verify"
  },
  "resolutions": {
    "prismjs": "^1.21.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@open-wc/rollup-plugin-html": "^1.2.4",
    "@open-wc/testing-helpers": "1.8.8",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@storybook/addon-controls": "6.0.22",
    "@storybook/addon-docs": "6.0.22",
    "@storybook/web-components": "6.0.22",
    "@types/expect-puppeteer": "^4.4.3",
    "@types/jest": "^26.0.14",
    "@types/jest-environment-puppeteer": "^4.3.2",
    "@types/puppeteer": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "@webcomponents/webcomponentsjs": "^2.4.4",
    "bundlesize": "^1.0.0-beta.2",
    "codecov": "^3.7.2",
    "concurrently": "^5.3.0",
    "del-cli": "^3.0.1",
    "eslint": "^7.10.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^24.0.2",
    "eslint-plugin-lit": "^1.2.2",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-wc": "^1.2.0",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "jest-junit": "^11.1.0",
    "jest-puppeteer": "^4.4.0",
    "lint-staged": "^10.4.0",
    "lit-analyzer": "^1.2.1",
    "lit-element": "^2.4.0",
    "lit-html": "^1.3.0",
    "prettier": "^2.1.2",
    "prettier-plugin-organize-imports": "^1.1.1",
    "prettier-plugin-packagejson": "^2.2.6",
    "puppeteer": "^5.3.1",
    "rollup": "^2.28.2",
    "rollup-plugin-minify-html-literals": "^1.2.5",
    "rollup-plugin-serve": "^1.0.4",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^26.4.0",
    "ts-loader": "^8.0.4",
    "type-fest": "^0.16.0",
    "typescript": "^4.0.3",
    "wait-on": "^5.2.0",
    "web-component-analyzer": "^1.1.6"
  },
  "peerDependencies": {
    "lit-element": "^2.4.0",
    "lit-html": "^1.3.0"
  },
  "bundlesize": [
    {
      "path": "hello-web-components.min.js",
      "maxSize": "1 kB"
    }
  ]
}
