{
  "name": "hello-web-components",
  "version": "1.2.0",
  "description": "A simple starter hello world web component written in typescript, using lit-html and lit-element",
  "keywords": [
    "hello-world",
    "lit-element",
    "lit-html",
    "starter",
    "web-components"
  ],
  "homepage": "https://github.com/fernandopasik/hello-web-components",
  "bugs": "https://github.com/fernandopasik/hello-web-components/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/fernandopasik/hello-web-components.git"
  },
  "license": "MIT",
  "author": "Fernando Pasik <fernando@pasik.com.ar> (https://fernandopasik.com)",
  "sideEffects": false,
  "type": "module",
  "main": "hello-web-components.js",
  "module": "hello-web-components.js",
  "typings": "hello-web-components.d.ts",
  "files": [
    "/lib",
    "/hello-web-components.*"
  ],
  "scripts": {
    "analyze": "wca analyze src --format json --outFile custom-elements.json",
    "prebuild": "del lib hello-web-components.* index.html",
    "build": "tsc && rollup -c",
    "check-types": "tsc --noEmit -p tsconfig.all.json",
    "clean": "del _site coverage lib hello-web-components.* index.html",
    "docs": "jekyll serve -s docs",
    "format": "prettier --write ** ./* ./.??*",
    "postinstall": "husky install",
    "lint": "eslint . --ext ts,js",
    "lit-analyze": "lit-analyzer src --strict",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "size": "bundlesize",
    "start": "start-storybook -p 6006",
    "test": "jest",
    "test:e2e": "concurrently -s first -k 'SERVE=true yarn build' 'wait-on http://localhost:10001 && jest'",
    "verify": "yarn lint && yarn lit-analyze && yarn check-types && yarn test && yarn test:e2e && yarn build && yarn size",
    "preversion": "yarn verify"
  },
  "resolutions": {
    "axios": "^0.21.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.3",
    "@commitlint/config-conventional": "^12.1.3",
    "@open-wc/rollup-plugin-html": "^1.2.5",
    "@open-wc/testing-helpers": "^1.8.12",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@storybook/addon-controls": "^6.1.21",
    "@storybook/addon-docs": "^6.1.21",
    "@storybook/web-components": "^6.1.21",
    "@types/expect-puppeteer": "^4.4.5",
    "@types/jest": "^26.0.23",
    "@types/jest-axe": "^3.5.1",
    "@types/jest-environment-puppeteer": "^4.4.1",
    "@types/puppeteer": "^5.4.3",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "@webcomponents/webcomponentsjs": "^2.5.0",
    "bundlesize": "^1.0.0-beta.2",
    "codecov": "^3.8.2",
    "concurrently": "^6.1.0",
    "del-cli": "^3.0.1",
    "eslint": "^7.26.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-lit": "^1.4.0",
    "eslint-plugin-lit-a11y": "^1.0.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-wc": "^1.3.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "jest-axe": "^4.1.0",
    "jest-junit": "^12.0.0",
    "jest-puppeteer": "^5.0.3",
    "lint-staged": "^11.0.0",
    "lit-analyzer": "^1.2.1",
    "lit-element": "^2.5.1",
    "lit-html": "^1.4.1",
    "pinst": "^2.1.6",
    "prettier": "^2.3.0",
    "prettier-plugin-organize-imports": "^2.0.0",
    "prettier-plugin-packagejson": "^2.2.11",
    "puppeteer": "^9.1.1",
    "rollup": "^2.47.0",
    "rollup-plugin-minify-html-literals": "^1.2.6",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^26.5.6",
    "ts-loader": "^9.1.2",
    "type-fest": "^1.1.1",
    "typescript": "^4.2.4",
    "wait-on": "^5.3.0",
    "web-component-analyzer": "^1.1.6"
  },
  "peerDependencies": {
    "lit-element": "^2.5.1",
    "lit-html": "^1.4.1"
  },
  "bundlesize": [
    {
      "path": "hello-web-components.min.js",
      "maxSize": "1 kB"
    }
  ]
}
