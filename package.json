{
  "name": "hello-web-components",
  "version": "1.3.9",
  "type": "module",
  "description": "A simple starter hello world web component written in typescript, using lit",
  "repository": "fernandopasik/hello-web-components",
  "homepage": "https://github.com/fernandopasik/hello-web-components",
  "bugs": "https://github.com/fernandopasik/hello-web-components/issues",
  "author": "Fernando Pasik <fernando@pasik.com.ar> (https://fernandopasik.com)",
  "license": "MIT",
  "main": "hello-web-components.js",
  "module": "hello-web-components.js",
  "typings": "hello-web-components.d.ts",
  "files": [
    "/hello-web-components.*",
    "/lib"
  ],
  "keywords": [
    "hello-world",
    "lit",
    "lit-element",
    "lit-html",
    "starter",
    "web-components"
  ],
  "scripts": {
    "analyze": "wca analyze src --format json --outFile custom-elements.json",
    "build": "tsc -p tsconfig.build.json && rollup -c",
    "check-types": "tsc --noEmit",
    "clean": "del _site coverage lib hello-web-components.* index.html",
    "docs": "jekyll serve -s docs",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint . --ext js,ts",
    "lit-analyze": "lit-analyzer src --strict",
    "postpublish": "pinst --enable",
    "prebuild": "del lib hello-web-components.* index.html",
    "prepare": "husky install",
    "prepublishOnly": "pinst --disable",
    "preversion": "npm run verify",
    "size": "bundlewatch",
    "start": "start-storybook -p 6006",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:e2e": "JEST_PUPPETEER_CONFIG=jest-puppeteer.config.cjs jest",
    "verify": "npm run format:check && npm run lint && npm run lit-analyze && npm run check-types && npm run test:coverage && npm run build && npm run size"
  },
  "peerDependencies": {
    "lit": "2.x.x"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@open-wc/rollup-plugin-html": "^1.2.5",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@storybook/addon-a11y": "^6.5.13",
    "@storybook/addon-essentials": "^6.5.13",
    "@storybook/web-components": "^6.5.13",
    "@types/expect-puppeteer": "^5.0.2",
    "@types/jest": "^27.5.0",
    "@types/jest-axe": "^3.5.5",
    "@types/jest-environment-puppeteer": "^5.0.3",
    "@types/react": "^18.0.25",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "@webcomponents/webcomponentsjs": "^2.7.0",
    "bundlewatch": "^0.3.3",
    "del-cli": "^5.0.0",
    "eslint": "^8.28.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.5",
    "eslint-plugin-lit": "^1.6.1",
    "eslint-plugin-lit-a11y": "^2.3.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-storybook": "^0.6.7",
    "eslint-plugin-wc": "^1.3.2",
    "husky": "^8.0.2",
    "jest": "^27.5.1",
    "jest-axe": "^7.0.0",
    "jest-environment-jsdom": "^27.5.1",
    "jest-junit": "^15.0.0",
    "jest-puppeteer": "^6.1.1",
    "lint-staged": "^13.0.3",
    "lit": "^2.4.1",
    "lit-analyzer": "^1.2.1",
    "pinst": "^3.0.0",
    "prettier": "^2.8.0",
    "prettier-plugin-organize-imports": "^3.2.0",
    "prettier-plugin-pkg": "^0.17.1",
    "prettier-plugin-sh": "^0.12.8",
    "puppeteer": "^13.7.0",
    "rollup": "^3.4.0",
    "rollup-plugin-minify-html-literals": "^1.2.6",
    "rollup-plugin-serve": "^2.0.1",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^27.1.4",
    "ts-lit-plugin": "^1.2.1",
    "ts-loader": "^9.4.1",
    "type-fest": "^3.2.0",
    "typescript": "^4.9.3",
    "wait-on": "^6.0.1",
    "web-component-analyzer": "^2.0.0-next.4"
  },
  "overrides": {
    "default-browser-id": "2.0.0",
    "glob-parent": "6.0.2",
    "rollup": "^3.4.0",
    "trim": "1.0.1"
  },
  "bundlewatch": {
    "files": [
      {
        "maxSize": "1 kB",
        "path": "hello-web-components.min.js"
      }
    ]
  }
}
