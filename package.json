{
  "name": "hello-web-components",
  "version": "0.0.0",
  "description": "A simple starter hello world web component written in typescript, using lit-html and lit-element",
  "keywords": [
    "hello-world",
    "lit-element",
    "lit-html",
    "starter",
    "web-components"
  ],
  "homepage": "https://github.com/fernandopasik/hello-web-components",
  "bugs": "https://github.com/fernandopasik/hello-web-components/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/fernandopasik/hello-web-components.git"
  },
  "license": "MIT",
  "author": "Fernando Pasik <fernando@pasik.com.ar> (https://fernandopasik.com)",
  "sideEffects": false,
  "type": "module",
  "main": "hello-world.js",
  "module": "hello-world.js",
  "typings": "hello-world.d.ts",
  "files": [
    "/hello-world.*"
  ],
  "scripts": {
    "analyze": "wca analyze src --format json --outFile custom-elements.json",
    "prebuild": "del lib hello-world.* index.html",
    "build": "tsc && rollup -c",
    "check-types": "tsc --noEmit -p tsconfig.all.json",
    "lint": "lit-analyzer src --strict && eslint . --ext ts,js",
    "size": "bundlesize",
    "start": "start-storybook -p 6006",
    "test": "jest",
    "test:e2e": "concurrently -s first -k 'SERVE=true yarn build' 'wait-on http://localhost:10001 && jest'",
    "verify": "yarn lint && yarn check-types && yarn test && yarn test:e2e && yarn build && yarn size",
    "preversion": "yarn verify"
  },
  "resolutions": {
    "conventional-changelog-angular": "^5.0.11",
    "conventional-changelog-conventionalcommits": "^4.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^9.1.2",
    "@open-wc/rollup-plugin-html": "^1.2.2",
    "@open-wc/testing-helpers": "^1.8.6",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "@storybook/addon-controls": "6.0.2",
    "@storybook/addon-docs": "6.0.2",
    "@storybook/web-components": "6.0.2",
    "@types/expect-puppeteer": "^4.4.3",
    "@types/jest": "^26.0.9",
    "@types/jest-environment-puppeteer": "^4.3.2",
    "@types/puppeteer": "^3.0.1",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.0",
    "@webcomponents/webcomponentsjs": "^2.4.4",
    "bundlesize": "^1.0.0-beta.2",
    "codecov": "^3.7.2",
    "concurrently": "^5.3.0",
    "del-cli": "^3.0.1",
    "eslint": "^7.6.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-lit": "^1.2.2",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "jest": "^26.3.0",
    "jest-junit": "^11.1.0",
    "jest-puppeteer": "^4.4.0",
    "lint-staged": "^10.2.11",
    "lit-analyzer": "^1.2.1",
    "lit-element": "^2.3.1",
    "lit-html": "^1.2.1",
    "prettier": "^2.0.5",
    "prettier-plugin-organize-imports": "^1.1.1",
    "prettier-plugin-packagejson": "^2.2.5",
    "puppeteer": "^5.2.1",
    "rollup": "^2.23.1",
    "rollup-plugin-minify-html-literals": "^1.2.4",
    "rollup-plugin-serve": "^1.0.3",
    "rollup-plugin-terser": "^6.1.0",
    "ts-jest": "^26.2.0",
    "ts-loader": "^8.0.2",
    "type-fest": "^0.16.0",
    "typescript": "^3.9.7",
    "wait-on": "^5.2.0",
    "web-component-analyzer": "^1.1.6"
  },
  "peerDependencies": {
    "lit-element": "^2.3.1",
    "lit-html": "^1.2.1"
  },
  "bundlesize": [
    {
      "path": "hello-world.min.js",
      "maxSize": "1 kB"
    }
  ]
}
