{
  "name": "hello-web-components",
  "version": "0.0.0",
  "description": "Declarative way of routing for lit-html powered by pwa-helpers, redux and lit-element",
  "keywords": [
    "lit-element",
    "lit-html",
    "hello-world"
  ],
  "homepage": "https://github.com/fernandopasik/hello-web-components",
  "bugs": "https://github.com/fernandopasik/hello-web-components/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/fernandopasik/hello-web-components.git"
  },
  "license": "MIT",
  "author": "Fernando Pasik <fernando@pasik.com.ar> (https://fernandopasik.com)",
  "sideEffects": false,
  "main": "hello-world.js",
  "module": "hello-world.js",
  "typings": "hello-world.d.ts",
  "files": [
    "/hello-world.*"
  ],
  "scripts": {
    "prebuild": "del lib hello-world.* index.html",
    "build": "tsc && rollup -c",
    "check-types": "tsc --noEmit -p tsconfig.all.json",
    "lint": "lit-analyzer src --strict && eslint . --ext ts,js",
    "size": "bundlesize",
    "start": "start-storybook -p 6006",
    "test": "jest",
    "test:e2e": "TESTS=E2E jest",
    "verify": "yarn lint && yarn check-types && yarn test && yarn test:e2e && yarn build && yarn size",
    "preversion": "yarn verify"
  },
  "resolutions": {
    "jsdom": "^16.2.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@rollup/plugin-html": "^0.1.1",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@storybook/web-components": "^5.3.18",
    "@types/expect-puppeteer": "^4.4.1",
    "@types/jest": "^25.2.1",
    "@types/jest-environment-puppeteer": "^4.3.1",
    "@types/puppeteer": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "@webcomponents/webcomponentsjs": "^2.4.3",
    "babel-loader": "^8.1.0",
    "bundlesize": "^0.18.0",
    "del-cli": "^3.0.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-lit": "^1.2.0",
    "eslint-plugin-prettier": "^3.1.2",
    "html-webpack-plugin": "^4.0.4",
    "husky": "^4.2.3",
    "jest": "^25.2.7",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "jest-puppeteer": "^4.4.0",
    "jsdom": "^16.2.2",
    "lint-staged": "^10.1.2",
    "lit-analyzer": "^1.1.10",
    "lit-element": "^2.3.1",
    "lit-html": "^1.2.1",
    "prettier": "^2.0.2",
    "puppeteer": "^2.1.1",
    "rollup": "^2.3.3",
    "rollup-plugin-minify-html-literals": "^1.2.3",
    "rollup-plugin-serve": "^1.0.1",
    "rollup-plugin-terser": "^5.3.0",
    "ts-jest": "25.2.1",
    "ts-loader": "^6.2.2",
    "typescript": "^3.8.3"
  },
  "peerDependencies": {
    "lit-element": "^2.3.1",
    "lit-html": "^1.2.1"
  },
  "bundlesize": [
    {
      "path": "hello-world.min.js",
      "maxSize": "1 kB"
    }
  ]
}
