{
  "name": "hello-web-components",
  "version": "1.4.0",
  "type": "module",
  "description": "A simple starter hello world web component written in typescript, using lit",
  "repository": "fernandopasik/hello-web-components",
  "homepage": "https://github.com/fernandopasik/hello-web-components",
  "bugs": "https://github.com/fernandopasik/hello-web-components/issues",
  "author": "Fernando Pasik <fernando@pasik.com.ar> (https://fernandopasik.com)",
  "license": "MIT",
  "main": "hello-web-components.js",
  "module": "hello-web-components.js",
  "typings": "hello-web-components.d.ts",
  "files": [
    "/hello-web-components.*",
    "/lib"
  ],
  "keywords": [
    "hello-world",
    "lit",
    "lit-element",
    "lit-html",
    "starter",
    "web-components"
  ],
  "scripts": {
    "analyze": "wca analyze src --format json --outFile custom-elements.json",
    "build": "tsc -p tsconfig.build.json && rollup -c",
    "check-types": "tsc --noEmit",
    "clean": "del _site coverage lib hello-web-components.* index.html",
    "docs": "jekyll serve -s docs",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint . --ext js,ts",
    "lit-analyze": "lit-analyzer src --strict",
    "prebuild": "del lib hello-web-components.* index.html",
    "prepare": "husky install",
    "preversion": "npm run verify",
    "size": "bundlewatch",
    "storybook": "storybook dev -p 6006",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:e2e": "JEST_PUPPETEER_CONFIG=jest-puppeteer.config.cjs jest",
    "verify": "npm run format:check && npm run lint && npm run lit-analyze && npm run check-types && npm run test:coverage && npm run build && npm run size"
  },
  "peerDependencies": {
    "lit": "3.x.x"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.0.0",
    "@commitlint/config-conventional": "^18.0.0",
    "@open-wc/rollup-plugin-html": "^1.2.5",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@storybook/addon-a11y": "^7.5.0",
    "@storybook/addon-essentials": "^7.5.0",
    "@storybook/web-components": "^7.5.0",
    "@storybook/web-components-vite": "^7.5.0",
    "@types/expect-puppeteer": "^5.0.4",
    "@types/jest": "^29.5.5",
    "@types/jest-axe": "^3.5.6",
    "@types/jest-environment-puppeteer": "^5.0.4",
    "@types/mime": "^3.0.2",
    "@types/react": "^18.2.28",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "@webcomponents/webcomponentsjs": "^2.8.0",
    "bundlewatch": "^0.3.3",
    "del-cli": "^5.1.0",
    "eslint": "^8.51.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jest": "^27.4.2",
    "eslint-plugin-lit": "^1.9.1",
    "eslint-plugin-lit-a11y": "^4.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-storybook": "^0.6.15",
    "eslint-plugin-wc": "^2.0.4",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-axe": "^8.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-junit": "^16.0.0",
    "jest-puppeteer": "^9.0.1",
    "lint-staged": "^15.0.1",
    "lit": "^3.0.0",
    "lit-analyzer": "^2.0.1",
    "prettier": "^3.0.3",
    "prettier-plugin-organize-imports": "^3.2.3",
    "prettier-plugin-pkg": "^0.18.0",
    "prettier-plugin-sh": "^0.13.1",
    "puppeteer": "^21.3.8",
    "rollup": "^4.1.4",
    "rollup-plugin-minify-html-literals": "^1.2.6",
    "rollup-plugin-serve": "^2.0.2",
    "storybook": "^7.5.0",
    "ts-jest": "^29.1.1",
    "ts-lit-plugin": "^2.0.0",
    "type-fest": "^4.4.0",
    "typescript": "^5.2.2",
    "vite": "^4.4.11",
    "wait-on": "^7.0.1",
    "web-component-analyzer": "^2.0.0-next.4"
  },
  "overrides": {
    "@open-wc/building-utils": "2.21.0",
    "glob-parent": "6.0.2",
    "rollup": "^4.1.4"
  },
  "bundlewatch": {
    "files": [
      {
        "maxSize": "1 kB",
        "path": "hello-web-components.min.js"
      }
    ]
  }
}
