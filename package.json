{
  "name": "hello-web-components",
  "version": "0.0.0",
  "description": "A simple starter hello world web component written in typescript, using lit-html and lit-element",
  "keywords": [
    "hello-world",
    "lit-element",
    "lit-html",
    "starter",
    "web-components"
  ],
  "homepage": "https://github.com/fernandopasik/hello-web-components",
  "bugs": "https://github.com/fernandopasik/hello-web-components/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/fernandopasik/hello-web-components.git"
  },
  "license": "MIT",
  "author": "Fernando Pasik <fernando@pasik.com.ar> (https://fernandopasik.com)",
  "sideEffects": false,
  "main": "hello-world.js",
  "module": "hello-world.js",
  "typings": "hello-world.d.ts",
  "files": [
    "/hello-world.*"
  ],
  "scripts": {
    "analyze": "wca analyze src --format json --outFile custom-elements.json",
    "prebuild": "del lib hello-world.* index.html",
    "build": "tsc && rollup -c",
    "check-types": "tsc --noEmit -p tsconfig.all.json",
    "lint": "lit-analyzer src --strict && eslint . --ext ts,js",
    "size": "bundlesize",
    "start": "start-storybook -p 6006",
    "test": "jest",
    "test:e2e": "TESTS=E2E jest",
    "verify": "yarn lint && yarn check-types && yarn test && yarn test:e2e && yarn build && yarn size",
    "preversion": "yarn verify"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@rollup/plugin-html": "^0.2.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@storybook/addon-knobs": "6.0.0-beta.6",
    "@storybook/web-components": "6.0.0-beta.6",
    "@types/expect-puppeteer": "^4.4.1",
    "@types/jest": "^25.2.2",
    "@types/jest-environment-puppeteer": "^4.3.1",
    "@types/puppeteer": "^2.1.0",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@webcomponents/webcomponentsjs": "^2.4.3",
    "bundlesize": "^0.18.0",
    "codecov": "^3.6.5",
    "del-cli": "^3.0.1",
    "eslint": "^7.0.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.13.0",
    "eslint-plugin-lit": "^1.2.0",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "jest-junit": "^10.0.0",
    "jest-puppeteer": "^4.4.0",
    "jsdom": "^16.2.2",
    "lint-staged": "^10.2.2",
    "lit-analyzer": "^1.1.10",
    "lit-element": "^2.3.1",
    "lit-html": "^1.2.1",
    "prettier": "^2.0.5",
    "puppeteer": "^3.0.4",
    "rollup": "^2.10.2",
    "rollup-plugin-minify-html-literals": "^1.2.3",
    "rollup-plugin-serve": "^1.0.1",
    "rollup-plugin-terser": "^5.3.0",
    "ts-jest": "^26.0.0",
    "ts-loader": "^7.0.4",
    "typescript": "^3.9.2",
    "web-component-analyzer": "^1.0.3"
  },
  "peerDependencies": {
    "lit-element": "^2.3.1",
    "lit-html": "^1.2.1"
  },
  "bundlesize": [
    {
      "path": "hello-world.min.js",
      "maxSize": "1 kB"
    }
  ]
}
